{
  "expo": {
    "name": "whatShop",
    "description": "This project is really great.",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/drawable/icon/shop-app-icon.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android":{
      "package": "com.eiu.whatShop",
      "permissions": [
        "CAMERA"
      ]
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
